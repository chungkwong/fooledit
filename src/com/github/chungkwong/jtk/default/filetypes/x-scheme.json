{
    "states":{"init":0,"comment1":1,"comment2":2,"comment3":3,"comment4":4},
	"shorthands":["delimiter","([\\(\\)\"\\|;\\s]|\\z)",
		"mnemonic","\\\\[abtrn]",
		"symbolChar","[^\\|\\\\]|\\\\([abtrn\"\\\\]|x[0-9a-fA-F]+)",
		"initial","[a-zA-Z!$%*/:<=>?^_~&\\u200C\\u200D\\p{Lu}\\p{Ll}\\p{Lt}\\p{Lm}\\p{Lo}\\p{Mn}\\p{Nl}\\p{No}\\p{Pd}\\p{Pc}\\p{Po}\\p{Sc}\\p{Sm}\\p{Sk}\\p{So}\\p{Co}]",
		"subsequent","[0-9@\\p{Nd}\\p{Mc}\\p{Me}\\.{initial}]",
		"peculiar","[-+]([-+@{initial}]{subsequent}*)?|[-+]?\\.[-+@\\.{initial}]{subsequent}*",
		"stringChar","[^\"\\\\]|\\\\([abtrn\"\\\\]|x[0-9a-fA-F]+|[ \\t]*(\\r|\\n|\\r\\n)[ \\t]*)",
		"infnan","[-+](inf|nan)\\.0",
	    "decimal","([0-9]+(\\.[0-9]*)|\\.[0-9]+)([eE][-+]?[0-9]+)?",
		"digit2","[01]","digit8","[0-7]","digit10","[0-9]","digit16","[0-9a-fA-F]",
		"ureal2","({digit2}+(/{digit2}+)?|{decimal})",
		"real2","([-+]?{ureal2}|{infnan})",
		"num2","(#[bB](#[ieIE])?|#[ieIE]#[bB])({real2}|{real2}[-+]{ureal2}?i|{real2}{infnan}i|{real2}@{real2}||[+-]{ureal2}i|{infnan}i|[-+]i)",
		"ureal8","({digit8}+(/{digit8}+)?|{decimal})",
		"real8","([-+]?{ureal8}|{infnan})",
		"num8","(#[oO](#[ieIE])?|#[ieIE]#[oO])({real8}|{real8}[-+]{ureal8}?i|{real8}{infnan}i|{real8}@{real8}||[+-]{ureal8}i|{infnan}i|[-+]i)",
		"ureal10","({digit10}+(/{digit10}+)?|{decimal})",
		"real10","([-+]?{ureal10}|{infnan})",
		"num10","(#[dD](#[ieIE])?|#[ieIE]#[dD])?({real10}|{real10}[-+]{ureal10}?i|{real10}{infnan}i|{real10}@{real10}||[+-]{ureal10}i|{infnan}i|[-+]i)",
		"ureal16","({digit16}+(/{digit16}+)?|{decimal})",
		"real16","([-+]?{ureal16}|{infnan})",
		"num16","(#[xX](#[ieIE])?|#[ieIE]#[xX])({real16}|{real16}[-+]{ureal16}?i|{real16}{infnan}i|{real16}@{real16}||[+-]{ureal16}i|{infnan}i|[-+]i)"],
	"rules":[
		{"old_state":"init","new_state":"init","type":"boolean","regex":"(#t|#f|#true|#false)(?={delimiter})"},
		{"old_state":"init","new_state":"init","type":"comment","regex":";[^\\r\\n]*|#;"},
		{"old_state":"init","new_state":"comment1","type":"comment","regex":"#\\|"},
		{"old_state":"comment1","new_state":"comment1","type":"comment","regex":"([^#|]|#[^|]|\\|[^#])+"},
	    {"old_state":"comment1","new_state":"init","type":"comment","regex":"\\|#"},
	    {"old_state":"comment1","new_state":"comment2","type":"comment","regex":"#\\|"},
		{"old_state":"comment2","new_state":"comment2","type":"comment","regex":"([^#|]|#[^|]|\\|[^#])+"},
	    {"old_state":"comment2","new_state":"comment1","type":"comment","regex":"\\|#"},
	    {"old_state":"comment2","new_state":"comment3","type":"comment","regex":"#\\|"},
		{"old_state":"comment3","new_state":"comment3","type":"comment","regex":"([^#|]|#[^|]|\\|[^#])+"},
	    {"old_state":"comment3","new_state":"comment2","type":"comment","regex":"\\|#"},
	    {"old_state":"comment3","new_state":"comment4","type":"comment","regex":"#\\|"},
		{"old_state":"comment4","new_state":"comment4","type":"comment","regex":"([^#|]|#[^|]|\\|[^#])+"},
	    {"old_state":"comment4","new_state":"comment3","type":"comment","regex":"\\|#"},
	    	{"old_state":"init","new_state":"init","type":"whitespace","regex":"\\s+"},
		{"old_state":"init","new_state":"init","type":"char","regex":"#\\\\(.|x[0-9a-fA-F]+|alarm|backspace|delete|escape|newline|null|return|space|tab)(?={delimiter})"},
		{"old_state":"init","new_state":"init","type":"string","regex":"\"({stringChar})*\""},
		{"old_state":"init","new_state":"init","type":"name","regex":"{initial}{subsequent}*|{peculiar}|\\|({symbolChar})*\\|"},
		{"old_state":"init","new_state":"init","type":"float","regex":"{num2}|{num8}|{num10}|{num16}"},
		{"old_state":"init","new_state":"init","type":"other","regex":"[\\(\\)'`,\\.]|#\\(|#u8\\(|,@"}
	]
}
